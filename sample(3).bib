  year={2017},
  volume={},
  number={},
  pages={1-6},
  abstract={The instructional methodologies that are applied in Computer and Telecommunication Engineering Education are outdated and contribute to student failure and attrition. A methodological framework which is considered appropriate, respecting individuality and enhancing (Gardner's) multiple intelligences is analysed and applied in an experimental course intelligences is analysed and applied in an experimental course which addresses academic/research skills development through project-based instruction. This piece of research is thus two-fold as it proposes a dual shift which involves new content (skills) and a modern mode of acquisition. The pedagogical considerations and learning objectives are outlined and the effectiveness of this approach is investigated through the evaluation of student projects submissions.},
  keywords={Computer science;Engineering education;Telecommunications;Collaboration;Team working;Europe;instructional methodologies;engineering education;Gardner’s multiple intelligence theory;project-based learning;active learning;academic/research skills},
  doi={10.1109/EAEEIE.2017.8768591},
  ISSN={2472-7687},
  month={June},}@ARTICLE{10476766,
  author={Tang, Hansheng and He, Ye and Wang, Xiaoming and Zheng, Hao and Xu, Bin and Zhao, Wenguang and Wu, Hongbin},
  journal={IEEE Transactions on Industry Applications}, 
  title={Two-Stage Multi-Mode Voltage Control for Distribution Networks: A Deep Reinforcement Learning Approach Based on Multiple Intelligences}, 
  year={2024},
  volume={60},
  number={4},
  pages={5681-5691},
  abstract={Photovoltaic output and load uncertainty are prone to cause voltage fluctuations in a distribution network, which can affect the power quality. With a particular focus on the distribution network voltage overrun caused by this problem, we propose a two-stage multi-mode voltage control strategy for distribution networks based on deep reinforcement learning to mitigate voltage fluctuations while realizing coordinated operation among different devices. First, the on-load regulator transformer and capacitor bank are effectively controlled using a centralized control strategy in the day-ahead phase to obtain the optimal tidal current for the distribution network, in order to achieve optimal operation of the long time-scale equipment of the system. Then, a distributed control strategy based on deep reinforcement learning with multiple intelligences is applied to regulate the reactive power of the photovoltaic inverters, based on local observations in the intraday phase, to provide fast, flexible, and reactive power support for the distribution network. Finally, a multi-mode data-driven conversion strategy based on deep reinforcement learning is used to coordinate and convert between different control modes, to enable the system to effectively reduce voltage offsets and reactive power losses under varying operational demand. The proposed method is experimentally validated on an improved IEEE 33-node distribution system, and the results show that it can effectively solve the voltage overrun problem and reduce the reactive power loss in the distribution system.},
  keywords={Voltage control;Distribution networks;Reactive power;Centralized control;Decentralized control;Optimization;Fluctuations;Deep reinforcement learning;mode conversion;multimodal;multiple intelligences;voltage control},
  doi={10.1109/TIA.2024.3379489},
  ISSN={1939-9367},
  month={July},}@INPROCEEDINGS{10532481,
  author={Rodvaree, Pheemmpong and Suyoteetanarat, Pichayapa and Naknut, Kittipoom and Sinthupinyo, Sukree and Malakul, Sivakorn},
  booktitle={2024 IEEE International Conference on Cybernetics and Innovations (ICCI)}, 
  title={A New Clustering Personality for Higher Education Selection Based on Multiple Intelligences}, 
  year={2024},
  volume={},
  number={},
  pages={1-6},
  abstract={This study aims to formulate a counseling model assisting high school students in making informed decisions regarding higher education based on their Multiple Intelligences (MI)-derived aptitudes. Data from 294 higher education students and graduates were collected and subsequently analyzed using K-Means clustering and Repeated Measurement ANOVA. The outcomes revealed the classification of participants into five distinct clusters, each exhibiting unique personality traits aligned with MI (F(4) = 262.89, p < 0.001, η2 = 0.35). Evaluation of the model's accuracy using K-fold Cross Validation with 5 fold yielded a result of 75.51%. The study further establishes a correlation between these groups and the aptitude characteristics of corresponding faculty groups. Consequently, the model discerns faculty trends suitable for individuals undergoing aptitude assessments, facilitating a more tailored approach to selecting a university for their continued education.},
  keywords={Employee welfare;Technological innovation;Engineering profession;Atmospheric measurements;Education;Particle measurements;Market research;Higher Education;Educational Guidance;Multiple Intelligences;Clustering;Personality},
  doi={10.1109/ICCI60780.2024.10532481},
  ISSN={},
  month={March},}@INPROCEEDINGS{7009652,
  author={Chang, Yi-Hsin and Lin, Yu-Kai},
  booktitle={2014 International Conference on Machine Learning and Cybernetics}, 
  title={A kinect-based system for learning Chinese cultural festivals}, 
  year={2014},
  volume={2},
  number={},
  pages={462-468},
  abstract={A Kinect-based learning system for educating learners on Chinese cultural festivals is developed in this study. The primary design principle involves integrating augmented reality with situated learning to allow users to learn through immersion into a story setting, increasing their interests in learning and improving their learning outcomes. The system features the following characteristics: (a) Key elements of situated learning theory are used to design learning activities and content (b) Augmented reality functions are used to integrate virtual characters into real-life settings. This allows for actual spatial and physical experiences, assisting users to fully immerse in the story and settings, thereby developing student motor and cognitive skills. The Lantern Festival is used as the learning objective of this study, and students from two classes of fourth-grade level at Shengli Elementary School in Tainan City are recruited as the study participant In the experiment, the students are divided into a control group receiving traditional teaching and an experimental group that employs the system developed in this study. Finally, in addition to evaluating questionnaires and multiple intelligences of students, this study aims to assess their pretest and posttest results for determining student learning outcomes.},
  keywords={Abstracts;Atmospheric measurements;Particle measurements;Web sites;Pragmatics;Kinect;Cultural festivals;Multiple intelligences;Situated learning},
  doi={10.1109/ICMLC.2014.7009652},
  ISSN={2160-1348},
  month={July},}@INPROCEEDINGS{10837648,
  author={Belfiore, Nicola Pio and Apollonia, Matrisciano},
  booktitle={2024 21st International Conference on Information Technology Based Higher Education and Training (ITHET)}, 
  title={Correlations between Academic Choices and Learning Preferences: Insights from an Experimental Study on Multiple Intelligences and Experiential Learning at Roma Tre University}, 
  year={2024},
  volume={},
  number={},
  pages={1-7},
  abstract={In this paper, Gardner's Multiple Intelligences inventory is applied within a high-level education technical context to identify distinctive learning preferences among students. The aim is to determine which students may benefit most from participating in Experiential Learning groups at the university level. This objective was achieved through the administration of a questionnaire to two groups of individuals. The first group consisted of students participating in Experiential Learning activities, while the second was a control group, composed of other students with the same background. The initial analysis involved a straightforward comparison of the responses given by the two groups, applying standard statistical analysis and the Student's t distribution hypothesis. Statistically significant characteristic differences with an appropriate confidence coefficient were identified. A se cond st udy fo cused on analyzing the correlation matrix among the dimensions examined, but limited to the group of interest. This analysis also revealed some correlations. Although this research is quantitatively limited, the insights it provides are quite interesting and align well with expectations regarding the goals of the Experiential Learning activities.},
  keywords={Training;Correlation;Statistical analysis;Engineering profession;Music;Linguistics;Information technology;Standards;multiple intelligence;orientation;experiential learning},
  doi={10.1109/ITHET61869.2024.10837648},
  ISSN={2473-2060},
  month={Nov},}@INPROCEEDINGS{10449520,
