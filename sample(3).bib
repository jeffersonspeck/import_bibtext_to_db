  month={Aug},}@INPROCEEDINGS{4738070,
  author={Hou, H. Y. and Hou, T. H. and Hu, H. J.},
  booktitle={2008 IEEE International Conference on Industrial Engineering and Engineering Management}, 
  title={Social intelligence in digital library guide designers}, 
  year={2008},
  volume={},
  number={},
  pages={1249-1252},
  abstract={Social intelligence plays an important role in multiple-intelligence. There is limited research about social intelligence evaluation in Taiwan. The goal of the present study was to adapt the Tromso Social Intelligence Scale (TSIS) to the digital library guide designers to evaluate the social intelligence in life skill. In addition to TSIS, computer self-efficiency scale was added to do related measurement. Besides, Kepner-Tregoe analysis (KTA) was used to collect designing scores and decide the final E-materials. Our analyses revealed good internal reliability, the correlation between TSIS and basic computer skill was high and applying KTA decided better digital library guide movies.},
  keywords={Software libraries;Motion pictures;Electronic learning;Content management;Industrial engineering;Technology management;Engineering management;Cognition;Information processing;Radio access networks;Social Intelligence;Computer Self-Efficiency;Kepner-Tregoe Analysis;Digital Library},
  doi={10.1109/IEEM.2008.4738070},
  ISSN={2157-362X},
  month={Dec},}@INPROCEEDINGS{10012735,
  author={Ji, Yuxin and Zhang, Xixi and Wang, Yu and Gacanin, Haris and Sari, Hikmet and Adachi, Fumiyuki and Gui, Guan},
  booktitle={2022 IEEE 96th Vehicular Technology Conference (VTC2022-Fall)}, 
  title={Multi-Agent Reinforcement Learning Aided Resources Allocation Method in Vehicular Networks}, 
  year={2022},
  volume={},
  number={},
  pages={1-5},
  abstract={To address the problem of spectrum resources and transmitting power for vehicular networks, this paper proposes a resource allocation (RA) method based on dueling double deep-Q network (D3QN) reinforcement learning (RL). Due to the high mobility of the vehicle, the channel changes rapidly which makes it difficult to accurately collect high-accuracy channel state information at the base station and to perform centralized management. In response of this difficulty, we construct a multi-intelligence model, using Manhattan Grid Layout City Model as the basis of environment and with each vehicle-to-vehicle (V2V) link as an intelligence. They work together to interact with the environment, receive appropriate observations, get rewards, and finally learn to improve the allocation of power and spectrum to enable users to achieve a better entertainment experience and a safer driving environment. Experimental results demonstrate that with proper training mechanism and reward function construction, cooperation among multiple intelligence can be performed in a distributed manner, with improvements in both the capacity of total vehicle-to-infrastructure links and the effective payload delivery success rate of the V2V links compared to common Q-network.},
  keywords={Training;Vehicular and wireless technologies;Vehicle-to-infrastructure;Simulation;Urban areas;Entertainment industry;Vehicular ad hoc networks;Vehicle networks;dueling double deep-Q network;multi-agent reinforcement learning;resources allocation},
  doi={10.1109/VTC2022-Fall57202.2022.10012735},
  ISSN={2577-2465},
  month={Sep.},}@INPROCEEDINGS{8901628,
  author={Noval, Bimaswara Adam and Safrodin, Mohamad and Hakkun, Rizky Yuniar},
  booktitle={2019 International Electronics Symposium (IES)}, 
  title={Battlebot : Logic Learning Based on Visual Programming Implementation in Multiplayer Game Online}, 
  year={2019},
  volume={},
  number={},
  pages={138-142},
  abstract={Logic is one of the multiple intelligence expressed by howard gardener. The problem is that many people are still having difficulty in knowing their problems, moreover find solutions to these problems. Along with the development of computer technology and communication. Learning through software and games is commonplace. Efforts to learn logic through the game media have already begun. One of the most popular is robocode. This game invites the player to design their own artificial intelligence to control the characters in the game. But the method used to make artificial intelligence is to use conventional programming languages. This certainly becomes an obstacle for ordinary people, because programming languages are not generally known. Especially the code syntax in it. Visual programming is a solutions for this problem, where people often use certain symbols in communication.},
  keywords={Logic;Visual Programming;Game},
  doi={10.1109/ELECSYM.2019.8901628},
  ISSN={},
  month={Sep.},}@INPROCEEDINGS{7463288,
  author={Alves dos Santos, Ranieri and Pozzebon, Eliane and Frigo, Luciana Bolan and Marcelino, Roderval},
  booktitle={2015 3rd Experiment International Conference (exp.at'15)}, 
  title={Remote experimentation model based on digital TV}, 
  year={2015},
  volume={},
  number={},
  pages={321-324},
  abstract={This work proposes the use of interactive digital TV as an interface for access to remote experimentation laboratories, because remote labs can fulfill the purposes of experiments in various areas of education, and digital TV can be used with interactive applications. The proposal described in this paper was based on the interactivity features of digital television systems, as well as on the particular aspects of remote experiments, underpinned by the theory of multiple intelligences and the patterns of interactivity of digital TV systems. The model resulting from this work guides the deployment of interactive TV applications to access remote experiments, and it seeks to provide individualized learning alternatives covering three Gardner's multiple intelligences in an interactive environment for remote experimentation based on using only the remote control, and bringing together the broadcast industry and the educational environment.},
  keywords={Digital TV;Streaming media;Laboratories;Education;Mobile handsets;Maintenance engineering;digital TV;remote experimentation;interactivity},
  doi={10.1109/EXPAT.2015.7463288},
  ISSN={},
  month={June},}@INPROCEEDINGS{10751471,
  author={Indrianti, Yasinta and Sasmoko and Manalu, Sonya Rapinta and Kurniawan, Lukas Tanto and Abu Bakar, Abu Yazid and Hermanus, Davy Ronald},
  booktitle={2024 International Conference on ICT for Smart Society (ICISS)}, 
  title={Self-Assessment for Personal Growth: Evaluating the Effectiveness of the BeeBest Mobile Application}, 
  year={2024},
  volume={},
  number={},
  pages={1-5},
  abstract={Self-assessment in personal growth is an important element in evaluating the effectiveness of mobile applications such as BeeBest, which are designed to support the concept of smart education by helping students with time management and self-development. This study evaluates the relationship between multiple intelligences, entrepreneurial awareness, and students' academic performance through the use of the BeeBest application. Using the SEM-PLS method, the results show that Intrapersonal Intelligence (coefficient 0.55) and Logic Intelligence (coefficient 0.45) have a significant influence on academic achievement. Entrepreneurial Mindfulness also shows a positive contribution (coefficient 0.35), reflecting the importance of focus, creativity, and stress resilience in supporting academic success. This model explains 60% of the variability in academic performance, reinforcing the importance of a personalized approach in smart education. The BeeBest application is proven to be effective in supporting self-management and academic growth through features that monitor student potential and provide learning recommendations based on intelligence and entrepreneurial profiles.},
  keywords={Entrepreneurship;Educational technology;Mobile applications;Logic;Stress;Monitoring;Creativity;Resilience;BeeBest Application;Entrepreneurial Mindfulness;P4AI;Self Assessment Personal Growth},
  doi={10.1109/ICISS62896.2024.10751471},
  ISSN={},
  month={Sep.},}@INPROCEEDINGS{10548210,
  author={Yu, Jian and Yang, Bifeng and Gu, Liwei and Chen, Ting and Zeng, Wenhao and Bian, Daisen and Liu, Chunguang},
  booktitle={2024 7th International Conference on Advanced Algorithms and Control Engineering (ICAACE)}, 
  title={Electric Vehicle Charging and Discharging Based on Multi-Agents Reinforcement Learning}, 
  year={2024},
  volume={},
  number={},
  pages={1070-1075},
  abstract={With the increase in the number of electric vehicles, the connection of EVs greatly increases the load on the power grid. By controlling the charging and discharging of EVs in a reasonable way, EVs can play a key role in the regulation of the power grid. Taking the charging and discharging control algorithm of EVs as the research object, we propose a deep deterministic policy gradient algorithm based on multiple intelligences to control the continuous charging and discharging behaviors of EVs. Aiming at the dynamic change of load timing in the distribution network, the charging and discharging power of different vehicles under different tariffs is controlled so that the users can get the maximum benefit. Considering the impact of EV charging on the distribution network, the optimal current is introduced to quantify the load caused by charging behavior on the network. Multiple EV systems in different environments are constructed as Markov decisions, and a multi-intelligence reinforcement learning algorithm is used to enable the vehicles to learn personalized charging and discharging strategies. The simulation results show that the algorithm can effectively regulate the charging and discharging behaviors of different vehicles and play the role of regulating the power grid, and the load caused by the algorithm on the power grid is more stable when the vehicles are charging.},
  keywords={Power demand;Heuristic algorithms;Simulation;Tariffs;Reinforcement learning;Distribution networks;Power grids;component;electric vehicles;charging control;deep deterministic policy gradient;grid regulation},
  doi={10.1109/ICAACE61206.2024.10548210},
  ISSN={},
  month={March},}@INPROCEEDINGS{10671441,
  author={He, Yingxue and Peng, Li},
  booktitle={2024 9th International Conference on Signal and Image Processing (ICSIP)}, 
  title={MAS-EGNN: A Multi-Agent EGNN-based Spectrum Allocation Optimization Framework for V2X Communications}, 
  year={2024},
  volume={},
  number={},
  pages={469-475},
  abstract={With the development of V2X technology, efficient spectrum resource management is critical to ensure the reliability and overall system performance of vehicle-to-vehicle communications. Traditional spectrum allocation methods often do not take into account inter-vehicle interference. In this paper, we introduce an innovative approach to eliminate interference in vehicle-to-vehicle communication, the MAS-EGNN framework. Initially, an Equivariant Graph Neural Networks (EGNN) is utilized to dynamically update the graph representation through node and edge conditions to effectively capture the relationships and dependencies between vehicles. Subsequently, multi-intelligence reinforcement learning techniques allow multiple intelligences to interact simultaneously within the environment, with each independently adapting to changes in the surrounding environment to optimize overall network performance. The effectiveness of the approach in improving communication quality and system throughput is verified through the simulation of V2X communication scenarios and the implementation of corresponding optimization strategies. The experimental results show that the method significantly reduces interference and optimizes V2X spectrum allocation compared with the traditional spectrum allocation strategy.},
  keywords={Adaptation models;Heuristic algorithms;System performance;Vehicular ad hoc networks;Interference;Reinforcement learning;Throughput;V2X Communication;Spectrum Resource Management;Equivariant Graph Neural Networks;Multi-Agent Reinforcement Learning},
  doi={10.1109/ICSIP61881.2024.10671441},
  ISSN={2642-6471},
  month={July},}@INPROCEEDINGS{9479537,
  author={Wang, Qingnan},
  booktitle={2021 2nd International Conference on Education, Knowledge and Information Management (ICEKIM)}, 
  title={The Creation of Multi Intelligence Music Classroom in Children's Enlightenment Stage Based on Virtual Reality Technology}, 
  year={2021},
  volume={},
  number={},
  pages={431-434},
  abstract={With the development of virtual reality technology, the real realization of virtual reality will cause great changes in human life and development. VR is the abbreviation of virtual reality, which means virtual reality in Chinese. Virtual reality is the ultimate application form of multimedia technology. It is the crystallization of the rapid development of computer hardware and software technology, sensor technology, robot technology, artificial intelligence and behavioral psychology. Because of this, the combination of VR and education classroom will become the inevitable trend of future education development. In the context of the development of traditional education, the enlightenment and influence of music education classroom on children is point like, and through the intervention and influence of VR technology, the influence of VR on music education begins to appear face like, and can maximize the mental and thinking ability of preschool children. Through the above analysis, this paper concludes the feasibility and importance of introducing VR technology into the current music education classroom, and will explore for the development of multiple intelligence education.},
  keywords={Education;Psychology;Virtual reality;Media;Robot sensing systems;Market research;Software;Vr multi intelligence music classroom;new media technology;interactive teaching scene;preschool education;curriculum system},
  doi={10.1109/ICEKIM52309.2021.00100},
  ISSN={},
