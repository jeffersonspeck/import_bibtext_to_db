  ISSN={},
  month={Sep.},}@INPROCEEDINGS{9793113,
  author={Sang, Xiaodan},
  booktitle={2022 International Conference on Applied Artificial Intelligence and Computing (ICAAIC)}, 
  title={Application of High-Performance MOOC Based on Multi-Center and Multi-Backup in the Design and Parallel Retrieval of a Combined Teaching Resource Library}, 
  year={2022},
  volume={},
  number={},
  pages={961-964},
  abstract={Based on the author's practice of setting up an “Information Retrieval” MOOC on iCourse.com, this paper discusses the new international Chinese teaching based on multiple intelligences theory and blended teaching method through classroom exercises, unit tests, final exams, and interactive assignments in MOOCs. The concept and characteristics of resource development. A cloud computing-based MOOC combined teaching resource data center backup system is proposed, which solves the problems of too many cloud storage interface styles and large files cannot be backed up to a certain extent. At the same time, it also brings improvements to the parallel retrieval of online teaching resources.},
  keywords={Computer aided instruction;Cloud computing;Data centers;Technological innovation;Electronic learning;Distance learning;Education;High-Performance MOOC;Multi-Center and Multi-Backup;Parallel Retrieval;Teaching Resource Library},
  doi={10.1109/ICAAIC53929.2022.9793113},
  ISSN={},
  month={May},}@INPROCEEDINGS{10729909,
  author={Ballaho, Jaydee C.},
  booktitle={2024 IEEE International Conference on Artificial Intelligence in Engineering and Technology (IICAIET)}, 
  title={Predicting Student's Success in Programming Courses: A Decision Support System for Admission in Computer Science and Information Technology Programs}, 
  year={2024},
  volume={},
  number={},
  pages={60-64},
  abstract={Predicting students' success prior to their admission to computer-related degree programs is challenging due to diverse educational backgrounds and varying skill requirements. Educational Data Mining (EDM) can be used to optimize the college admission process by selecting only well-equipped students for the program. The attrition rate of the College of Computing Studies at Western Mindanao State University remains high despite the rigorous admission process for the Computer Science and Information Technology programs. Many students fail programming courses, causing them to leave the program. This study developed a decision support system and ensemble models by combining Support Vector Machine, Decision Tree, and Neural Network models using learning techniques such as voting, bagging, and stacking. The goal is to automate the admission of pre-qualified students based on their likelihood of success in programming courses. Factors such as SHSGPA, CET score, class rank, and specific personality traits significantly influence academic success. The combined models, particularly using stacking techniques, emerged as the most reliable and effective model for predicting unseen data compared to standalone classification models. Students and faculty members evaluated the functionality, reliability, and usability of the decision-support system. The feedback indicated a positive reception, affirming its potential as a valuable tool in the college's admission process. Future enhancements may include incorporating multiple intelligence and grit tests and exploring additional ensemble methods to improve predictive capabilities.},
  keywords={Decision support systems;Support vector machines;Computational modeling;Stacking;Predictive models;Data models;Reliability;Ensemble learning;Information technology;Programming profession;college admissions;decision-support system;educational data mining;ensemble models;programming performance prediction},
  doi={10.1109/IICAIET62352.2024.10729909},
  ISSN={},
  month={Aug},}@INPROCEEDINGS{5632311,
  author={Chuang, T. Y. and You, J. H. and Duo, A.},
  booktitle={IET International Conference on Frontier Computing. Theory, Technologies and Applications}, 
  title={Digital game design principles for spatial ability enhancement}, 
  year={2010},
  volume={},
  number={},
  pages={122-127},
  abstract={The concepts of spatial visualization and the geometrical calculation are important elements of multiple-intelligence development. According to the cognitive-development theory of Piaget, the students in 5th and 6th grade in the elementary school are on a level of concrete thinking, in which their thinking may have greater flexible alternatives, and begin to develop conservation concepts, including physical, spatial ability, and numerical conservation. For this reason, the training of the spatial ability is imperative for students around these ages. This study is investigated the possibility of utilizes a digital game to improve individual's spatial abilities. An action game is designed and developed to achieve the purpose of the study. The game uses the 3D Studio Max 9.0 to build 3-D models, Photoshop CS2 for drawing characters and scenes textures, and Virtools Dev 4.0 for developing the game. The tasks of this game are designed to increase spatial ability with rotating question, visual discrimination and visual memory with space, rotate, reverse, inference and displacement. This study provides a game design model of various entities for learners' contact, depict size, shape, aspect and position. After we completed the game, we applied experimental activities at two primary schools. From the observations and questionnaire results, we found out that this game can increase students' motivation and willingness in learning. Players also were immersed in this game and followed the instructional materials we apply integrated into this game.},
  keywords={},
  doi={10.1049/cp.2010.0548},
  ISSN={},
  month={Aug},}@INPROCEEDINGS{7403504,
  author={Nee, Chee Ken and Ibrahim, Nor Hasniza and Yahaya, Noraffandy},
  booktitle={2015 IEEE Conference on e-Learning, e-Management and e-Services (IC3e)}, 
  title={Designing an iMLeaming apps as formulated ideal personalised social collaborative mobile learning environment}, 
  year={2015},
  volume={},
  number={},
  pages={153-157},
  abstract={This article presents the design of m-learning apps namely iMLearning as Formulated Ideal Personalised Social Collaborative Mobile Learning Environment (FIPSCOMOLE) based on several theories included social learning theory, collaborative learning theory, multiple intelligence theory and personalized learning theory as well. To this extent, we described the architecture and all the fuctionalities. The iMLearning application's key features include: video or audio communication between peer or instructor, watch educational video, read news feed, reading notes, playing games, and so forth primarily learning without stay static in a place and time. The application is designed on Android platform for mobile devices. To that end, this study aimed to design an effective m-learning apps and will then verified its usability, usefulness and effectiveness for m-learning in future research. Researcher proposed further towards future recommendations for future research in tis paper.},
  keywords={Mobile communication;Education;Smart phones;Collaboration;Multimedia communication;Collaborative work;mobile learning;mobile apps;collaborative learning;personalised learning;social learning},
  doi={10.1109/IC3e.2015.7403504},
  ISSN={},
  month={Aug},}@INPROCEEDINGS{5593588,
  author={Xuefen, Lin and Gang, Cen},
  booktitle={2010 5th International Conference on Computer Science & Education}, 
  title={Theoretical framework research of open-ended project-based instruction}, 
  year={2010},
  volume={},
  number={},
  pages={438-441},
  abstract={Open-ended project-based instruction is a theory and practice model to deepen project-based instruction. The paper proposed open-ended project-based instruction model based on theories of constructivism and multiple intelligences. And theoretical framework has been summarized: two centers, three basic conditions and four key processes. The steps about project selection, team composition, and process management have been analyzed actively. This will promote further spread of this instruction model.},
  keywords={Technological innovation;Educational institutions;Collaboration;Computational modeling;Adaptation model;Engineering education;Instructional Mode;Practice Teaching;Project-based Instruction;Open-ended Projet},
  doi={10.1109/ICCSE.2010.5593588},
  ISSN={},
  month={Aug},}@INPROCEEDINGS{10594697,
  author={Liu, Lei},
  booktitle={2024 IEEE 4th International Conference on Electronic Technology, Communication and Information (ICETCI)}, 
  title={Application of Multilink Scheduling Algorithm for PPO-based WBAN Systems}, 
  year={2024},
  volume={},
  number={},
  pages={204-208},
  abstract={With the increasing growth of the Internet of Things (IoT), WBAN (wireless body area network), as a kind of IoT, has attracted much attention. However, due to the problems of existing MAC protocols like poor flexibility and generalizability, a PPO (Proximal Policy Optimization)-based multilink scheduling algorithm with multiple intelligences called PM- WBAN is proposed in this paper. Firstly, based on the multiantenna technology and inter-coupling technology in the existing wearable field, we suggest a design of sensors with dual antennas that can work in dual frequency bands and adapt the flexible scheduling of the hub. Then, a Multi-Agent is proposed to optimize the multi-link scheduling within Intra-WBAN and the number of health packets sent by the sensors. Finally, in order to respond for the emergent traffic, PPO is introduced to improve the frame time length of slot-ALOHA and an adapted reward function is designed. Experiments verify that PM-WBAN can converge quickly. The results also demonstrate that PM-WBAN outperforms the benchmark algorithms such as Random, Max-Rate, and it is more adaptable for those scenarios with random load as well.},
  keywords={Wireless communication;Wireless sensor networks;Scheduling algorithms;Optimal scheduling;Benchmark testing;Body area networks;Scheduling;IoT;WBAN;PPO;MAC},
  doi={10.1109/ICETCI61221.2024.10594697},
  ISSN={},
  month={May},}
